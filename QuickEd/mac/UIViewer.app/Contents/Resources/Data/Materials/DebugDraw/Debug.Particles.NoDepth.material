Material:
    Layers: ["TransclucentRenderLayer"]
    Shader: "~res:/Materials/Shaders/Default/particle-materials"
    UniqueDefines: ["MATERIAL_TEXTURE", "ALPHABLEND", "VERTEX_COLOR"]
    RenderState:
        blend: true
    Passes:
        Forward:
        ForwardWithDepthOnly:
        ReflectionRefraction:
        DepthOnly:
            enabled: false
        GBuffer:
            enabled: false
        ShadowMap:
            enabled: false
